{
  "name": "fabic/nql",
  "description": "NQL is not GraphQL, but some sort of GraphQL-like impl. for the Rest of us.",
  "keywords": ["composer", "package", "Laravel", "GraphQL", "REST"],
  "type": "library",
  "license": "MIT",
  "homepage": "http://fabic.net",
  "authors": [{"name": "Fabien Cadet", "email": "cadet.fabien@gmail.com",
               "homepage": "http://fabic.net", "role": "Developer" } ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Fabic\\Nql\\": "src"
    },
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Fabic\\Nql\\": "tests"
    }
  },
  "require": {
    "php": ">=7.0",
    "doctrine/lexer": "^1.0.1"
  },
  "require-dev": {
  },
  "suggests": {
    "laravel/framework": "^5.4.0",
    "doctrine/cache": "^1.6.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "\\Fabic\\Nql\\Laravel\\NqlServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
